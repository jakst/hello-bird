{
  "name": "hello-bird-server",
  "version": "1.0.0",
  "license": "MIT",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development tsx src/run.ts",
    "auth": "NODE_ENV=production tsx src/bot/auth.ts",
    "pre-build": "if [ -z ${NO_DIST_REPLACE+x} ]; then sed -i '' 's|src/lib.ts|dist/lib.js|' ../lib/package.json; fi",
    "build": "yarn pre-build && etsc",
    "start": "NODE_ENV=production node dist/run.js"
  },
  "dependencies": {
    "@fastify/cors": "^8.2.0",
    "@trpc/server": "^10.0.0-rc.6",
    "dotenv": "^16.0.3",
    "fastify": "^4.9.2",
    "fastify-sse-v2": "^2.2.1",
    "hello-bird-lib": "1.0.0",
    "nanoid": "^4.0.0",
    "puppeteer": "^19.2.2",
    "puppeteer-extra": "^3.3.4",
    "puppeteer-extra-plugin-stealth": "^2.11.1",
    "redis": "^4.5.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "esbuild": "^0.15.15",
    "esbuild-node-tsc": "^2.0.4",
    "tsx": "^3.11.0",
    "typescript": "^4.9.3"
  }
}
